msgctxt "#31083"
msgid "Combined"
msgstr "Combinado"

#: /1080i/Includes_Labels.xml
msgctxt "#31084"
msgid "Dynamically loaded menu items allow real time editing directly from the home screen using the context menu."
msgstr "Os itens de menu carregados dinamicamente permitem a edição em tempo real diretamente da tela inicial usando o menu de contexto."

msgctxt "#31085"
msgid "Live TV"
msgstr "TV Ao Vivo"

msgctxt "#31086"
msgid "Customise options"
msgstr "Opções de personalização"


		<width>450</width>
		<height>60</height>
		<radiowidth>120</radiowidth>
		<radioheight>76</radioheight>
		<textureradioonfocus colordiffuse="$VAR[ThemeLabelColor]">android/btn_radio_on_holo_dark.png</textureradioonfocus>
		<textureradioonnofocus colordiffuse="$VAR[ThemeLabelColor]">android/btn_radio_on_holo_dark.png</textureradioonnofocus>
		<textureradioofffocus colordiffuse="grey">android/btn_radio_off_holo_dark.png</textureradioofffocus>
		<textureradiooffnofocus colordiffuse="grey">android/btn_radio_off_holo_dark.png</textureradiooffnofocus>
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">lf_classic/button/button-focus2.png</texturefocus>
		<textureradioondisabled colordiffuse="40FFFFFF">android/btn_radio_on_holo_dark.png</textureradioondisabled>
		<textureradiooffdisabled colordiffuse="40FFFFFF">android/btn_radio_off_holo_dark.png</textureradiooffdisabled>

		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>grey3</disabledcolor>
		<textoffsetx>15</textoffsetx>
		<aligny>center</aligny>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="slider">
		<orientation>horizontal</orientation>
	</default>
	<default type="sliderex">
		<height>60</height>
		<textoffsetx>15</textoffsetx>
		<aligny>center</aligny>
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">lf_classic/button/button-focus2.png</texturefocus>
		<sliderwidth>150</sliderwidth>
		<sliderheight>20</sliderheight>
		<texturesliderbar>osd_slider_bg.png</texturesliderbar>
		<textureslidernib>osd_slider_nibNF.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[ThemeLabelColor]">osd_slider_nib.png</textureslidernibfocus>
		<font>font14</font>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>grey3</disabledcolor>
		<pulseonselect>false</pulseonselect>
		<scrollspeed>60</scrollspeed>
	</default>
	<default type="mover">
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">lf_classic/button/button-focus2.png</texturefocus>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="resize">
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">lf_classic/button/button-focus2.png</texturefocus>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="edit">
		<width>450</width>
		<height>60</height>
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">lf_classic/button/button-focus2.png</texturefocus>
		<font>font14</font>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>grey3</disabledcolor>
		<invalidcolor>invalid</invalidcolor>

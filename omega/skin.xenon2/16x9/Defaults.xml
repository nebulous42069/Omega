<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<default type="image">
		<texture />
	</default>
    <default type="selectbutton">
        <left>735</left>
        <top>502</top>
        <width>450</width>
        <height>75</height>
        <label></label>
        <font>font12</font>
        <texturefocus></texturefocus>
        <texturenofocus></texturenofocus>
        <texturebg colordiffuse="$VAR[ThemeLabelColor]">button-focus.png</texturebg>
        <textureleft>scroll-left.png</textureleft>
        <textureleftfocus colordiffuse="$VAR[ThemeLabelColor]">scroll-left-focus.png</textureleftfocus>
        <textureright>scroll-right.png</textureright>
        <texturerightfocus colordiffuse="$VAR[ThemeLabelColor]">scroll-right-focus.png</texturerightfocus>
        <textcolor>white</textcolor>
        <disabledcolor>grey3</disabledcolor>
        <aligny>center</aligny>
    </default>	
	<default type="multiimage">
		<aspectratio align="center">scale</aspectratio>
		<fadetime>FanartCrossfadeTime</fadetime>
		<include>FullscreenDimensions</include>
		<randomize>true</randomize>
	</default>
	<default type="label">
		<align>left</align>
		<aligny>center</aligny>
        <font>font13</font>
        <textcolor>white</textcolor>
        <disabledcolor>grey3</disabledcolor>
    </default>
	<default type="fadelabel">
		<font>font13</font>
		<textcolor>white</textcolor>
		<disabledcolor>grey3</disabledcolor>
		<scrollspeed>60</scrollspeed>
	</default>
    <default type="button">
        <width>450</width>
        <height>63</height>
        <include>NormalButton</include>
        <font>font13</font>
        <textcolor>white</textcolor>
        <disabledcolor>grey3</disabledcolor>
        <textoffsetx>10</textoffsetx>
        <aligny>center</aligny>
        <pulseonselect>no</pulseonselect>
    </default>
	<default type="togglebutton">
		<width>30</width>
		<height>60</height>
		<textoffsetx>15</textoffsetx>
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">lf_classic/button/button-focus2.png</texturefocus>
		<alttexturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">lf_classic/button/button-focus2.png</alttexturefocus>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>grey3</disabledcolor>
		<label />
		<aligny>center</aligny>
		<font>font14</font>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="textbox">
		<width>615</width>
		<height>322</height>
		<align>left</align>
		<textcolor>white</textcolor>
		<font>font15_textbox</font>
	</default>
	<default type="progress">
		<lefttexture />
		<righttexture />
		<overlaytexture />
        <midtexture border="6,0,6,0" colordiffuse="$VAR[ThemeOSDProgressColor]">OSDProgressMid.png</midtexture>
        <texturebg border="6,0,6,0" colordiffuse="$VAR[ThemeOSDProgressColor]">OSDProgressBack.png</texturebg>
	</default>
	<default type="spincontrol">
		<textureup colordiffuse="grey">android/numberpicker_up_normal_holo_dark.png</textureup>
		<texturedown colordiffuse="grey">android/numberpicker_down_normal_holo_dark.png</texturedown>
		<textureupfocus colordiffuse="$VAR[HighlightBarColor]">android/numberpicker_up_normal_holo_dark.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[HighlightBarColor]">android/numberpicker_down_normal_holo_dark.png</texturedownfocus>
		<texturedowndisabled colordiffuse="40FFFFFF">android/numberpicker_down_normal_holo_dark.png</texturedowndisabled>
		<textureupdisabled colordiffuse="40FFFFFF">android/numberpicker_up_normal_holo_dark.png</textureupdisabled>
		<align>right</align>
		<width>40</width>
		<height>25</height>
		<font>font14</font>
		<reverse>yes</reverse>
		<textcolor>grey</textcolor>
		<disabledcolor>grey3</disabledcolor>
		<subtype>text</subtype>
		<textoffsetx>0</textoffsetx>
		<textoffsety>0</textoffsety>
		<aligny>center</aligny>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="spincontrolex">
		<width>450</width>
		<height>60</height>
		<spinwidth>40</spinwidth>
		<spinheight>25</spinheight>

<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>T-V</name>
    <match>any</match>
    <rule field="title" operator="startswith">
        <value>T</value>
    </rule>
    <rule field="title" operator="startswith">
        <value>U</value>
    </rule>
    <rule field="title" operator="startswith">
        <value>V</value>
    </rule>
    <order direction="ascending">title</order>
</smartplaylist>
